{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-plusplus":"off",
        "no-param-reassign":"off",
        "no-undef":"off",
    }
}
